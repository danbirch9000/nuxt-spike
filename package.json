    "node-sass": "^4.7.2",
    "nuxt": "^1.0.0",
    "sass-loader": "^6.0.7",
    "vue-highcharts": "0.0.10",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "financejs": "^4.1.0",
    "jest": "^22.4.3",
    "jest-vue-preprocessor": "^1.4.0",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
